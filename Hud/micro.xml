<?xml version="1.0"?>
<!--
Learjet 35-A Micro HUD
PH-JBO 20111214p
-->
<PropertyList>
	<name>Micro Aircraft Hud</name>
	<enable3d>false</enable3d>

	<gauge>
		<name>Aileron</name>
		<x>-235</x>
		<y>-185</y>
		<width>50</width>
		<height>20</height>
		<option>notext</option>
		<option>bottom</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>true</cap-top>
		<cap-right>false</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<input>
			<property>/controls/flight/aileron</property>
			<max>1.0</max>
			<min>-1.0</min>
		</input>
	</gauge>
	<gauge>
		<name>Rudder</name>
		<x>-235</x>
		<y>-215</y>
		<width>50</width>
		<height>20</height>
		<option>top</option>
		<option>notext</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>true</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>false</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<input>
			<property>/controls/flight/rudder</property>
			<max>1.0</max>
			<min>-1.0</min>
		</input>
	</gauge>
	<gauge>
		<name>Gear</name>
		<x>-185</x>
		<y>-215</y>
		<width>10</width>
		<height>50</height>
		<option>vertical</option>
		<option>left</option>
		<option>notext</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
		<marker-offset>0.0</marker-offset>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>false</cap-right>
		<cap-left>true</cap-left>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<input>
			<property>/gear/gear/position-norm</property>
			<factor>-1</factor>
			<max>0</max>
			<min>-1</min>
		</input>
	</gauge>
	<gauge>
		<name>Flaps</name>
		<x>-185</x>
		<y>-215</y>
		<width>10</width>
		<height>50</height>
		<option>vertical</option>
		<option>right</option>
		<option>notext</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
		<marker-offset>0.0</marker-offset>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>false</cap-right>
		<cap-left>true</cap-left>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<input>
			<property>/controls/flight/flaps</property>
			<factor>-1</factor>
			<max>0</max>
			<min>-1</min>
		</input>
	</gauge>
	<gauge>
		<name>Elevator</name>
		<x>-235</x>
		<y>-215</y>
		<width>20</width>
		<height>50</height>
		<option>vertical</option>
		<option>right</option>
		<option>notext</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
